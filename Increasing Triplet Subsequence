#include <stdbool.h>
#include <limits.h>

bool increasingTriplet(int* nums, int numsSize) {
    int first = INT_MAX;
    int second = INT_MAX;

    for (int i = 0; i < numsSize; i++) {
        int num = nums[i];
        if (num <= first) {
            first = num;
        } else if (num <= second) {
            second = num;
        } else {
            return true; // found triplet
        }
    }
    return false;
}
